function sidebar_img(){
    $ip = $_SERVER['REMOTE_ADDR'];
    $db_result = db_query("SELECT * From frame_temp where ip_address='" . $ip . "'")->fetchAll();
    foreach($db_result as $v){
                   echo '<li class="ui-draggable">
   <img rel="../../sites/all/modules/frames/frames_temp/'.$v->image_name.'" src="../../sites/all/modules/frames/frames_temp/'.$v->image_name.'" id="'.$v->temp_id.'" class="img-thumbnail" width="50" height="50">
                   <div class="c-image-remove" title="Remove image"></div>
                   </li>';                  
                    }
}
