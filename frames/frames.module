function frames_menu() {
    $items = array();
     $items['frames/%'] = array(
        'title'             =>  'Frames View',  //page title
        'description'       =>  'Frames Information',  //description show when mouse hover on link
        'page callback'     =>  'frames_list',  //callback function which is invoked when menu item is called.
        'access callback'   =>  true,  //any user can access this page
       );
     $items['frames/show/%'] = array(
        'title' => t('collage picture gallery'),
        'page callback' => 'collage',
        'page arguments' => array(2),
        'access arguments' => array('access frame display'),
          'access callback'   =>  true,  //any user can access this page
         
    );
     return $items;
} 
function frames_list($cat) {
    if($cat>0){
        $result = db_query("SELECT * FROM frames where frames_category_id='" . $cat . "'")->fetchAll();   
    }
    else{
        $result = db_query("SELECT * FROM frames")->fetchAll();
    }
$output = theme('frames_list', array('data'=> $result)); 
return $output;
}
